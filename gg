                while math.round(task.wait(0.01)) do
                    cc()
                    chithcuthhfj()
                    cecek()
                    iqui()
                    Ffsnn()
                end
            end
        )
    end
)
spawn(
    function()
        while wait() do
            if World2 or World3 then
                AuraHaki()
