    local UsageTime = os.time() - StartTimeStats
    local Data = {
        executor = ExecutorName,
        player = PlayerName,
        usageTime = UsageTime,
        timestamp = os.time()
    }
    local Url = "https://sh-stats.onrender.com/"
    if SendHttpRequest(Url, Data) then
        print("say oh yeah")
    end
end
